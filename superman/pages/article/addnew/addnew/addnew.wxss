.html-box{
  margin-top:10rpx;
  margin-left: 22rpx;
  padding:20rpx 0;
  display:flex;
  background-color:#FFFFFF;
  justify-content:center;
  align-items:top;
  font-size:30rpx;
  color:#353535;
  line-height:2em;
  border-radius:10rpx;
  width:95%;
  border:1rpx solid #b02923;
}
.htm_add_top{
  color:#fff;
  width:70rpx;
  height:44rpx;
  line-height:45rpx;
  text-align:center;
  margin-top:2rpx;
  padding-left:25rpx;
  border-radius:8rpx;
}
.htm_add{
   float:left;
margin-left:40%;
  color:#fff;
  width:70rpx;
  height:44rpx;
  line-height:45rpx;
  text-align:center;
  margin-top:2rpx;
  padding-left:25rpx;
  border-radius:8rpx;
  margin-bottom:20rpx;
}
.htm_remove{
  float:left;
margin-left:4%;
color:#fff;
  width:70rpx;
  height:44rpx;
  line-height:45rpx;
  text-align:center;
  margin-top:2rpx;
  padding-left:25rpx;
  border-radius:8rpx;
  margin-bottom:20rpx;
}
.view_hide{
 display: none;
}
.view_show{
 display: block;
}

.menuWrap{
  width:95%;
  margin:10rpx auto;
  display: -webkit-box;
	display: -webkit-flex;
  display: flex;
  border:1rpx dotted #b02923;
  border-radius:8rpx;
}
.menuWrap-item{
  text-align: center;
  -webkit-box-flex: 1;
	-webkit-flex: 1;
	flex:1;
}
.menuWrap-item-navi{
  display: inline-block;
  width: 150rpx;
  color: #b02923;
}
.menuWrap-item-navi span{
  font-size: 30px;
}
.menuWrap-item-navi text{
  display: block;
}

.noticeList-item-r{
  padding-left: 8px;
  -webkit-box-flex: 1;
	-webkit-flex: 1;
	flex:1;
  overflow: hidden;
}
.noticeList-item-r text{
  display: block;
  font-size: 17px;
}
button{
    background-color: #b02923;
    color:#fff;
    width:100%;
    margin-top:5px;
    border-radius:0px;
}
.bottom_btn{
  position: fixed;
  bottom: 0;
  width: 100%;
}
